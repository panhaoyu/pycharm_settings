<application>
  <component name="ApplicationTasksOptions">
    <global-watchers>
      <TaskOptions>
        <option name="arguments" value="-m PyQt5.uic.pyuic  $FileName$ -o $FileNameWithoutExtension$.py" />
        <option name="checkSyntaxErrors" value="true" />
        <option name="description" />
        <option name="exitCodeBehavior" value="ERROR" />
        <option name="fileExtension" value="ui" />
        <option name="immediateSync" value="true" />
        <option name="name" value="UIC" />
        <option name="output" value="" />
        <option name="outputFilters">
          <array />
        </option>
        <option name="outputFromStdout" value="false" />
        <option name="program" value="$USER_HOME$/AppData/Local/Programs/Python/Python37/python.exe" />
        <option name="runOnExternalChanges" value="true" />
        <option name="scopeName" value="Project Files" />
        <option name="trackOnlyRoot" value="false" />
        <option name="workingDir" value="$FileDir$" />
        <envs />
      </TaskOptions>
    </global-watchers>
  </component>
</application>